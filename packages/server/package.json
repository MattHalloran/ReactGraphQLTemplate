{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup-prisma": "yarn prisma introspect && yarn prisma generate",
    "start-development": "nodemon --experimental-modules --experimental-specifier-resolution=node ./src/index.js",
    "start-production": "node --experimental-modules --experimental-specifier-resolution=node ./src/index.js",
    "migrate-create": "knex migrate:make --knexfile ./src/db/knexfile.js --esm",
    "seed-make": "knex seed:make --knexfile ./src/db/knexfile.js --esm",
    "seed-run": "knex seed:run --knexfile ./src/db/knexfile.js --specific init.js --esm",
    "migrate-latest": "knex migrate:latest --env development --knexfile ./src/db/knexfile.js --esm",
    "migrate-rollback": "knex migrate:rollback --knexfile ./src/db/knexfile.js --esm",
    "clean": "node --experimental-modules --experimental-specifier-resolution=node ./tools/cleanImageData.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@local/shared": "1.0.0",
    "@paljs/plugins": "^3.8.0",
    "@prisma/client": "^2.30.2",
    "@types/express": "^4.17.11",
    "@types/node": "^15.0.2",
    "apollo-server-express": "^2.25.0",
    "arrify": "^3.0.0",
    "bcrypt": "^5.0.1",
    "bull": "^3.22.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-upload": "^12.0.0",
    "heic-convert": "^1.2.4",
    "imghash": "^0.0.9",
    "jsonwebtoken": "^8.5.1",
    "knex": "0.95.5",
    "lodash": "^4.17.21",
    "nodemailer": "^6.6.0",
    "pg": "^8.7.1",
    "probe-image-size": "^7.2.1",
    "sharp": "^0.28.3",
    "subscriptions-transport-ws": "^0.9.19",
    "twilio": "^3.61.0",
    "xlsx": "^0.17.0"
  }
}
